{
  "login": {
    "modeSelect": {
      "systemUiOverlayStyle": {
        "statusBarIconBrightness": "dark",
        "statusBarBrightness": "light",
        "systemNavigationBarColor": "#000000",
        "systemNavigationBarIconBrightness": "light"
      },
      "mainLogo": {
        "uri": "asset://assets/images/primary_onboarding_logo.svg",
        "render": {
          "scale": 0.42,
          "padding": {
            "left": 0.0,
            "top": 0.0,
            "right": 0.0,
            "bottom": 0.0
          }
        }
      },
      "buttonLoginStyleType": "neutralOnDark",
      "buttonSignupStyleType": "neutralOnDark"
    },
    "switchPage": {
      "mainLogo": {
        "uri": "asset://assets/images/secondary_onboarding_logo.svg",
        "render": {
          "scale": 0.25,
          "padding": {
            "left": 0.0,
            "top": 0.0,
            "right": 0.0,
            "bottom": 0.0
          }
        }
      }
    },
    "otpSignin": {
      "_refTextField": {
        "keyboardType": "phone",
        "textAlign": "left",
        "decoration": {
          "labelText": "login_TextFieldLabelText_otpSigninUserRef",
          "__labelText": "Text used as the label or prefix in the TextField. Use an ARB key to enable translation. If the key is missing or cannot be resolved, the default label for this input will be used automatically. If omitted entirely, the value is treated as plain text for single-language setups.",
          "prefixText": "123 ",
          "hintText": "XXXXXXXXX"
        },
        "_mask": {
          "pattern": "+380 (##) ###-##-##",
          "filter": {
            "#": "[0-9]"
          }
        }
      }
    },
    "passwordSignin": {
      "_refTextField": {
        "keyboardType": "phone",
        "textAlign": "left",
        "decoration": {
          "labelText": "login_TextFieldLabelText_passwordSigninUserRef",
          "__labelText": "Text used as the label or prefix in the TextField. Use an ARB key to enable translation. If the key is missing or cannot be resolved, the default label for this input will be used automatically. If omitted entirely, the value is treated as plain text for single-language setups.",
          "prefixText": "123 ",
          "hintText": "XXXXXXXXX"
        },
        "_mask": {
          "pattern": "+380 (##) ###-##-##",
          "filter": {
            "#": "[0-9]"
          }
        }
      }
    },
    "otpSigninVerify": {
      "countdownRepeatIntervalSeconds": 30
    },
    "signupVerify": {
      "countdownRepeatIntervalSeconds": 30
    }
  },
  "dialing": {
